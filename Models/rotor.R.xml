<?xml version="1.0"?>
<PropertyList>

 <path>rotor.ac</path>


 <animation>
  <type>spin</type>
  <object-name>blade.1</object-name>
  <object-name>blade.2</object-name>
  <object-name>blade.3</object-name>
  <object-name>hinge.1</object-name>
  <object-name>hinge.2</object-name>
  <object-name>hinge.3</object-name>
  <object-name>rotor</object-name>
  <property>rotors/right/rpm</property>
  <center>
   <x-m>0</x-m>
   <y-m>0</y-m>
   <z-m>0</z-m>
  </center>
  <axis>
   <x>0</x>
   <y>0</y>
   <z>-1</z>
  </axis>
 </animation>
 <animation>
  <type>rotate</type>
  <object-name>blade.1</object-name>
  <property>rotors/right/cone-deg</property>

  <axis>
   <x>0</x>
   <y>1</y>
   <z>0</z>
  </axis>
 </animation>
</PropertyList>
