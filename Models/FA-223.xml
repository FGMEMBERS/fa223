<?xml version="1.0"?>

  <PropertyList> 
    <path>fa223.ac</path>
		
	<model>
		<name>cockpit</name>
		<path>Aircraft/fa223/Models/cockpit.xml</path>
	</model>
	<animation>
  <object-name>cockpit</object-name>
		<type>range</type>
		<min-m>0</min-m>
		<max-m>10</max-m>
	</animation>
	<model>
		<name>left rotor</name>
		<path>Aircraft/fa223/Models/rotor.L.xml</path>
  <offsets>
   <x-m>-0.843</x-m>
   <y-m>-6.282</y-m>
   <z-m>2.6</z-m>
  </offsets>
	</model>
	<model>
		<name>right rotor</name>
		<path>Aircraft/fa223/Models/rotor.R.xml</path>
  <offsets>
   <x-m>-0.843</x-m>
   <y-m>6.282</y-m>
   <z-m>2.6</z-m>
  </offsets>
	</model>
	<animation>
		<type>shader</type>
		<shader>chrome</shader>
		<texture>Aircraft/fa223/Models/glass_shader.png</texture>
		<object-name>cockpitglas</object-name>
	</animation>

 <animation>
  <type>rotate</type>
  <object-name>FlightPanel</object-name>
    <object-name>PanelHolder</object-name>
  <property>controls/flightpanel/heading-deg</property>
  <center>
   <x-m>-3.925</x-m>
   <y-m>0</y-m>
   <z-m>-0.097</z-m>
  </center>
  <axis>
   <x>0</x>
   <y>0</y>
   <z>-1</z>
  </axis>
 </animation>
  <animation>
    <type>pick</type>
    <object-name>PanelHolder</object-name>
    <visible>true</visible>
    <action>
      <button>3</button>
      <repeatable>true</repeatable>
      <binding>
        <command>property-adjust</command>
        <property>controls/flightpanel/heading-deg</property>
        <step>-1.0</step>
        <min>0</min>
        <max>60</max>
        <wrap>0</wrap>
      </binding>
    </action>
 </animation>
  <animation>
    <type>pick</type>
    <object-name>PanelHolder</object-name>
    <visible>true</visible>
    <action>
      <button>4</button>
      <repeatable>true</repeatable>
      <binding>
        <command>property-adjust</command>
        <property>controls/flightpanel/heading-deg</property>
        <step>1.0</step>
        <min>0</min>
        <max>60</max>
        <wrap>0</wrap>
      </binding>
    </action>
 </animation>
 <animation>
  <type>rotate</type>
    <object-name>FlightPanel</object-name>
  <property>controls/flightpanel/pitch-deg</property>
  <center>
   <x-m>-3.87</x-m>
   <y-m>0</y-m>
   <z-m>0.097</z-m>
  </center>
  <axis>
   <x>0</x>
   <y>-1</y>
   <z>0</z>
  </axis>
 </animation>
  <animation>
    <type>pick</type>
    <object-name>FlightPanel</object-name>
    <visible>true</visible>
    <action>
      <button>3</button>
      <repeatable>true</repeatable>
      <binding>
        <command>property-adjust</command>
        <property>controls/flightpanel/pitch-deg</property>
        <step>1.0</step>
        <min>0</min>
        <max>60</max>
        <wrap>0</wrap>
      </binding>
    </action>
 </animation>
  <animation>
    <type>pick</type>
    <object-name>FlightPanel</object-name>
    <visible>true</visible>
    <action>
      <button>4</button>
      <repeatable>true</repeatable>
      <binding>
        <command>property-adjust</command>
        <property>controls/flightpanel/pitch-deg</property>
        <step>-1.0</step>
        <min>0</min>
        <max>60</max>
        <wrap>0</wrap>
      </binding>
    </action>
 </animation>

  </PropertyList>
